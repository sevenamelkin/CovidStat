{
    "http":
    {
        "Host" : "localhost",
        "Port" : 12666
    },
    "Serilog" : {
        "Using": [],
        "MinimumLevel": {
            "Default" : "Verbose",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "Enrich": [
            "WithMachineName"
        ],
        "WriteTo": [
            {
                "Name" : "Console" },
            {
                "Name" : "File",
                "Args" : {
                    "path" : "log\\main.log",
                    "outputTemplate" : "{Timestamp:G} [{Level:u3}] {Message}{NewLine:1}{Exception:1}"
                }
            }
        ]
    }
}